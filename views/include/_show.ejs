

<%for(var i=0;i<users.length;i++){%>
  <tr>
    <td><%=i+1%></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].specie%></td>
    <td><%=users[i].gender%></td>
    <td><%=users[i].vaccine%></td>
    <td>
      <!-- update -->
      <a href="/update-user/<%=users[i]._id%>" class="btn border-shadow update">
        <span class="text-gradient">
          <i class="fas fa-pencil-alt"></i>
        </span>
      </a>

      <!-- delete -->
      <a data-id="<%= users[i]._id%>" class="btn border-shadow delete">
        <span class="text-gradient">
          <i class="fas fa-times"></i>
        </span>
      </a>
    </td>
  </tr>
  <%}%></users.length;i++){%
>
